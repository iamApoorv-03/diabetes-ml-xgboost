    "# Run this cell in a notebook to create folders\n",
    "import os\n",
    "\n",
    "base = \"diab_app\"\n",
    "subfolders = [\"models\", \"api\", \"streamlit\", \"assets\"]\n",
    "\n",
    "os.makedirs(base, exist_ok=True)\n",
    "for s in subfolders:\n",
    "    os.makedirs(os.path.join(base, s), exist_ok=True)\n",
    "\n",
    "print(\"Folders created under:\", base)\n"
   ]
